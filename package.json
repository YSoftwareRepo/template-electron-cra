{
    "name": "template-electron-create-react-app",
    "version": "0.0.0",
    "description": "A template project for developing electron application by electron, react (create-react-app) and typescript.",
    "repository": "git@github.com:YF1999/template-electron-create-react-app.git",
    "author": {
        "name": "YF1999",
        "email": "1586939317@qq.com",
        "url": "https://github.com/YF1999"
    },
    "private": true,
    "main": "build/electron.js",
    "homepage": "./",
    "scripts": {
        "app": "electron .",
        "build": "yarn build-renderer && yarn build-main",
        "build-dev": "webpack --config configs/webpack.main.config.js --mode development",
        "build-main": "webpack --config configs/webpack.main.config.js --mode production",
        "build-renderer": "react-app-rewired build",
        "eject": "react-scripts eject",
        "installer": "electron-builder build --config ./configs/electron-builder.yml -w --x64",
        "main": "yarn build-dev && cross-env mode=development electron .",
        "test": "jest --config configs/jest.config.js",
        "test-react": "react-app-rewired test",
        "web": "react-app-rewired start"
    },
    "eslintConfig": {
        "extends": "./configs/eslintrc.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "config-overrides-path": "configs/webpack.renderer.config.js",
    "dependencies": {
        "@reduxjs/toolkit": "^1.4.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.1",
        "react-scripts": "3.4.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/jest": "^24.0.0",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "@types/react-redux": "^7.1.9",
        "cross-env": "^7.0.2",
        "electron": "10.1.1",
        "electron-builder": "^22.7.0",
        "react-app-rewired": "^2.1.6",
        "ts-jest": "^26.1.3",
        "ts-loader": "^8.0.1",
        "typescript": "^3.9.7",
        "webpack-cli": "^3.3.12"
    }
}