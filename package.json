{
    "name": "tecra",
    "version": "0.6.0",
    "description": "A template project for developing electron application by electron, react (CRA) and typescript.",
    "main": "build/electron.js",
    "homepage": "https://github.com/YSoftwareRepo/template-electron-cra#readme",
    "license": "MIT",
    "author": {
        "name": "yifanwww",
        "email": "yifanw1101@gmail.com",
        "url": "https://github.com/yifanwww"
    },
    "bugs": {
        "url": "https://github.com/YSoftwareRepo/template-electron-cra/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YSoftwareRepo/template-electron-cra.git"
    },
    "scripts": {
        "build": "build-packages && npm run build:renderer && npm run build:main",
        "build:main": "npm run build --workspace @tecra/electron-main",
        "build:renderer": "npm run build --workspace @tecra/electron-renderer",
        "build:renderer-profile": "npm run build-profile --workspace @tecra/electron-renderer",
        "build-packages": "build-packages",
        "build-profile": "build-packages && npm run build:renderer-profile && npm run build:main",
        "build-scripts": "npm run build-scripts --workspace @tecra/scripts",
        "clean": "rimraf build release working && npm run clean:packages",
        "clean:build": "rimraf build",
        "clean:packages": "npm run clean --workspaces",
        "clean:release": "rimraf release",
        "clean:working": "rimraf working",
        "clean-scripts": "npm run clean-scripts --workspace @tecra/scripts",
        "dev": "build-packages && concurrently -n renderer,main \"npm run dev:renderer\" \"npm run dev:main\"",
        "dev:main": "npm run dev --workspace @tecra/electron-main",
        "dev:renderer": "npm run dev --workspace @tecra/electron-renderer",
        "format": "npm run format:root && npm run format:packages",
        "format:packages": "npm run format --workspaces",
        "format:root": "format-code",
        "gen-installer": "electron-builder build --config configs/electron-builder.json --win --publish never",
        "lint": "npm run lint --workspaces",
        "prebuild": "npm run build-scripts && mkdir-working",
        "predev": "npm run build-scripts && mkdir-working",
        "run-build": "run-build",
        "run-unpacked": "run-unpacked",
        "test-coverage": "npm run test-coverage --workspaces"
    },
    "engines": {
        "node": ">=16"
    },
    "workspaces": [
        "packages/assets",
        "packages/electron-common",
        "packages/electron-main",
        "packages/electron-renderer",
        "packages/eslint-config",
        "packages/global-types",
        "packages/hooks",
        "packages/scripts",
        "packages/stylelint-config",
        "packages/tsconfigs",
        "packages/utils-fluentui",
        "packages/utils-redux"
    ],
    "dependencies": {
        "@fluentui/react": "^8.28.2",
        "@reduxjs/toolkit": "^1.6.1",
        "error-oop": "^0.4.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4",
        "react-scrollbars-custom": "^4.0.27",
        "web-vitals": "^2.1.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^7.0.1",
        "@testing-library/user-event": "^13.2.1",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/jest": "^26.0.24",
        "@types/lodash": "^4.14.172",
        "@types/node": "^14.17.9",
        "@types/react": "^17.0.18",
        "@types/react-dom": "^17.0.9",
        "@types/react-redux": "^7.1.18",
        "@types/react-test-renderer": "^17.0.1",
        "@types/speed-measure-webpack-plugin": "^1.3.4",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/experimental-utils": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "@typescript-eslint/scope-manager": "^4.30.0",
        "@typescript-eslint/types": "^4.30.0",
        "@typescript-eslint/typescript-estree": "^4.30.0",
        "@typescript-eslint/visitor-keys": "^4.30.0",
        "browserslist": "^4.16.7",
        "chalk": "^4.1.2",
        "concurrently": "^6.2.1",
        "electron": "13.1.7",
        "electron-builder": "^22.11.7",
        "electron-devtools-installer": "^3.2.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-typescript": "^14.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-deprecation": "^1.2.1",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.25.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "lodash": "^4.17.21",
        "prettier": "2.3.2",
        "react-app-rewired": "^2.1.8",
        "react-scripts": "^4.0.3",
        "react-test-renderer": "^17.0.2",
        "rimraf": "^3.0.2",
        "sass": "^1.38.0",
        "speed-measure-webpack-plugin": "^1.5.0",
        "stylelint": "^13.13.1",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "ts-loader": "^8.3.0",
        "typed-css-modules": "^0.6.8",
        "typescript": "4.4.2",
        "typescript-plugin-css-modules": "^3.4.0",
        "webpack-cli": "^4.8.0"
    }
}
