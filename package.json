{
    "name": "tecra",
    "version": "0.8.0",
    "description": "A template project for developing electron application by electron, react (CRA) and typescript.",
    "main": "build/electron.js",
    "homepage": "https://github.com/YSoftwareRepo/template-electron-cra#readme",
    "license": "MIT",
    "author": "yifanwww <yifanw1101@gmail.com> (https://github.com/yifanwww)",
    "bugs": {
        "url": "https://github.com/YSoftwareRepo/template-electron-cra/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YSoftwareRepo/template-electron-cra.git"
    },
    "scripts": {
        "build": "npm run build:app",
        "build:app": "npm run build:renderer && npm run build:main",
        "build:main": "npm run build --workspace @tecra/electron-main",
        "build:renderer": "npm run build --workspace @tecra/electron-renderer",
        "build:renderer-profile": "npm run build-profile --workspace @tecra/electron-renderer",
        "build-packages": "build-packages",
        "build-profile": "build-packages && npm run build:renderer-profile && npm run build:main",
        "build-scripts": "npm run build-scripts --workspace @tecra/scripts",
        "clean": "rimraf build release working && npm run clean:packages",
        "clean:build": "rimraf build",
        "clean:packages": "npm run clean --workspaces --if-present",
        "clean:release": "rimraf release",
        "clean:working": "rimraf working",
        "clean-scripts": "npm run clean-scripts --workspace @tecra/scripts",
        "dev": "npm run dev:app",
        "dev:app": "concurrently -n renderer,main \"npm run dev:renderer\" \"npm run dev:main\"",
        "dev:main": "npm run dev --workspace @tecra/electron-main",
        "dev:renderer": "npm run dev --workspace @tecra/electron-renderer",
        "format": "npm run format:root && npm run format:packages",
        "format:packages": "npm run format --workspaces --if-present",
        "format:root": "format-code",
        "gen-installer": "electron-builder build --config configs/electron-builder.json --win --publish never",
        "lint": "npm run lint --workspaces --if-present",
        "pre-commit": "npm run pre-commit --workspaces --if-present",
        "prebuild": "npm run build-scripts && mkdir-working && build-packages",
        "predev": "npm run build-scripts && mkdir-working && build-packages",
        "prepare": "husky install",
        "run-build": "run-build",
        "run-unpacked": "run-unpacked",
        "test-full": "npm run test-full --workspaces --if-present"
    },
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "@fluentui/react": "^8.87.2",
        "@reduxjs/toolkit": "^1.8.4",
        "clsx": "^1.2.1",
        "error-oop": "^0.6.0",
        "immer": "^9.0.15",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^8.0.2",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "react-scrollbars-custom": "^4.1.0",
        "ts-essentials": "^9.2.0",
        "use-immer": "^0.6.0",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/case-sensitive-paths-webpack-plugin": "^2.1.6",
        "@types/electron-devtools-installer": "^2.2.2",
        "@types/jest": "^27.5.2",
        "@types/lodash": "^4.14.182",
        "@types/node": "^16.11.48",
        "@types/react": "^17.0.48",
        "@types/react-dom": "^17.0.17",
        "@types/react-test-renderer": "^17.0.2",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/experimental-utils": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "@typescript-eslint/scope-manager": "^5.33.0",
        "@typescript-eslint/types": "^5.33.0",
        "@typescript-eslint/typescript-estree": "^5.33.0",
        "@typescript-eslint/utils": "^5.33.0",
        "@typescript-eslint/visitor-keys": "^5.33.0",
        "babel-jest": "^27.5.1",
        "browserslist": "^4.21.3",
        "chalk": "^4.1.2",
        "concurrently": "^7.3.0",
        "electron": "19.0.11",
        "electron-builder": "^22.14.13",
        "electron-devtools-installer": "^3.2.0",
        "eslint": "^8.21.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^16.2.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^25.7.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.1",
        "jest": "^27.5.1",
        "lint-staged": "^11.2.6",
        "lodash": "^4.17.21",
        "prettier": "2.7.1",
        "react-app-rewired": "2.1.9",
        "react-scripts": "5.0.1",
        "react-test-renderer": "^17.0.2",
        "rimraf": "^3.0.2",
        "sass": "^1.54.4",
        "stylelint": "^14.10.0",
        "stylelint-config-recess-order": "^3.0.0",
        "stylelint-config-sass-guidelines": "^9.0.1",
        "typescript": "4.7.4",
        "typescript-plugin-css-modules": "^3.4.0",
        "webpack-cli": "^4.10.0"
    },
    "workspaces": [
        "packages/assets",
        "packages/electron-common",
        "packages/electron-main",
        "packages/electron-renderer",
        "packages/eslint-config",
        "packages/global-types",
        "packages/hooks",
        "packages/scripts",
        "packages/stylelint-config",
        "packages/tsconfigs",
        "packages/utils-fluentui",
        "packages/utils-react",
        "packages/utils-redux",
        "packages/utils-test",
        "packages/utils-type"
    ]
}
