{
    "name": "template-electron-create-react-app",
    "version": "0.0.0",
    "description": "A template project for developing electron application by electron, react (create-react-app) and typescript.",
    "repository": "git@github.com:YF1999/template-electron-create-react-app.git",
    "author": {
        "name": "YF1999",
        "email": "1586939317@qq.com",
        "url": "https://github.com/YF1999"
    },
    "private": true,
    "main": "public/electron.js",
    "homepage": "./",
    "scripts": {
        "prestart": "npm run build",
        "start": "electron .",
        "build": "npm run build-renderer",
        "build-renderer": "react-scripts build",
        "dev": "nodemon --config nodemon.json",
        "dev-electron": "cross-env mode=development electron .",
        "web": "react-scripts start",
        "test": "react-scripts test",
        "preinstaller": "npm run build",
        "installer": "electron-builder build -w"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/jest": "^24.0.0",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.1",
        "typescript": "~3.7.2"
    },
    "devDependencies": {
        "cross-env": "^7.0.2",
        "electron": "^9.1.0",
        "electron-builder": "^22.7.0",
        "nodemon": "^2.0.4"
    }
}