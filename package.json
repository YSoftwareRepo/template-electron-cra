{
    "name": "@ysoftwarerepo/template-electron-cra",
    "version": "0.5.0",
    "description": "A template project for developing electron application by electron, react (CRA) and typescript.",
    "main": "build/electron.js",
    "homepage": "https://github.com/YSoftwareRepo/template-electron-cra#readme",
    "license": "MIT",
    "author": {
        "name": "YF1999",
        "email": "1586939317@qq.com",
        "url": "https://github.com/YF1999"
    },
    "bugs": {
        "url": "https://github.com/YSoftwareRepo/template-electron-cra/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YSoftwareRepo/template-electron-cra.git"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "scripts": {
        "_cra-build": "react-app-rewired build --config-overrides scripts/webpack-configs/webpack.renderer.config.js",
        "_cra-dev": "react-app-rewired start --config-overrides scripts/webpack-configs/webpack.renderer.config.js",
        "_cra-test": "react-app-rewired test --config-overrides scripts/webpack-configs/webpack.renderer.config.js",
        "_main-build": "webpack --config scripts/webpack-configs/webpack.main.config.js --mode production",
        "_main-dev": "webpack --config scripts/webpack-configs/webpack.main.config.js --mode development",
        "build": "npm run _cra-build && npm run _main-build",
        "build:main": "npm run _main-build",
        "build:renderer": "npm run _cra-build",
        "build-profile": "npm run _cra-build -- --profile && npm run _main-build",
        "build-scripts": "tsc --build scripts/tsconfigs/tsconfig.scripts.json",
        "clean": "rimraf build release scripts/**/*.js scripts/**/*.map working **/*.tsbuildinfo",
        "clean:build": "rimraf build",
        "clean:release": "rimraf release",
        "clean:scripts": "rimraf scripts/**/*.js scripts/**/*.map scripts/tsconfigs/tsconfig.scripts.tsbuildinfo",
        "clean:working": "rimraf working",
        "dev": "concurrently -n renderer,main \"npm run _cra-dev\" \"npm run _main-dev\"",
        "dev:main": "npm run _main-dev",
        "dev:renderer": "npm run _cra-dev",
        "format": "prettier --write \"**/*.{cjs,css,html,js,jsx,mjs,json,scss,ts,tsx,yaml,yml}\"",
        "gen-installer": "electron-builder build --config scripts/electron/electron-builder.json --win",
        "lint": "eslint .",
        "prebuild": "npm run build-scripts && node scripts/md-working.js",
        "predev": "npm run build-scripts && node scripts/md-working.js",
        "run-build": "node scripts/run-app.js",
        "test": "npm run _cra-test -- --coverage",
        "test-coverage": "npm run _cra-test -- --coverage --watchAll=false"
    },
    "engines": {
        "node": ">=14 <15"
    },
    "dependencies": {
        "@fluentui/react": "^8.21.1",
        "@reduxjs/toolkit": "^1.6.0",
        "node-errors-pro": "^0.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4",
        "react-scrollbars-custom": "^4.0.25",
        "web-vitals": "^2.0.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^7.0.1",
        "@testing-library/user-event": "^13.1.9",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.170",
        "@types/node": "^14.17.4",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.8",
        "@types/react-redux": "^7.1.16",
        "@types/react-test-renderer": "^17.0.1",
        "@types/speed-measure-webpack-plugin": "^1.3.3",
        "@typescript-eslint/eslint-plugin": "^4.28.1",
        "@typescript-eslint/experimental-utils": "^4.28.1",
        "@typescript-eslint/parser": "^4.28.1",
        "@typescript-eslint/scope-manager": "^4.28.1",
        "@typescript-eslint/types": "^4.28.1",
        "@typescript-eslint/typescript-estree": "^4.28.1",
        "@typescript-eslint/visitor-keys": "^4.28.1",
        "chalk": "^4.1.1",
        "concurrently": "^6.2.0",
        "electron": "13.1.7",
        "electron-builder": "^22.11.7",
        "electron-devtools-installer": "^3.2.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-deprecation": "^1.2.1",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "lodash": "^4.17.21",
        "prettier": "2.3.2",
        "react-app-rewired": "^2.1.8",
        "react-scripts": "^4.0.3",
        "react-test-renderer": "^17.0.2",
        "rimraf": "^3.0.2",
        "sass": "^1.35.1",
        "speed-measure-webpack-plugin": "^1.5.0",
        "ts-loader": "^8.3.0",
        "typed-css-modules": "^0.6.8",
        "typescript": "4.3.5",
        "webpack-cli": "^4.7.2"
    }
}
