{
    "name": "tecra",
    "version": "0.6.0",
    "description": "A template project for developing electron application by electron, react (CRA) and typescript.",
    "main": "build/electron.js",
    "homepage": "https://github.com/YSoftwareRepo/template-electron-cra#readme",
    "license": "MIT",
    "author": {
        "name": "YF1999",
        "email": "yifanw1101@gmail.com",
        "url": "https://github.com/YF1999"
    },
    "bugs": {
        "url": "https://github.com/YSoftwareRepo/template-electron-cra/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YSoftwareRepo/template-electron-cra.git"
    },
    "browserslist": [
        "last 9 chrome versions"
    ],
    "scripts": {
        "_cra-build": "react-app-rewired build --config-overrides scripts/webpack-configs/webpack.renderer.config.js",
        "_cra-dev": "react-app-rewired start --config-overrides scripts/webpack-configs/webpack.renderer.config.js",
        "_cra-test": "react-app-rewired test --config-overrides scripts/webpack-configs/webpack.renderer.config.js",
        "_main-build": "webpack --config scripts/webpack-configs/webpack.main.config.js --mode production",
        "_main-dev": "webpack --config scripts/webpack-configs/webpack.main.config.js --mode development",
        "build": "npm run _cra-build && npm run _main-build",
        "build:main": "npm run _main-build",
        "build:renderer": "npm run _cra-build",
        "build-profile": "npm run _cra-build -- --profile && npm run _main-build",
        "build-scripts": "tsc --build scripts/tsconfig.json && npm run build-scripts --workspace @tecra/scripts",
        "clean": "rimraf build release working && npm run clean:packages",
        "clean:build": "rimraf build",
        "clean:packages": "npm run clean --workspaces",
        "clean:release": "rimraf release",
        "clean:working": "rimraf working",
        "clean-scripts": "rimraf scripts/**/*.js scripts/**/*.map scripts/tsconfig.tsbuildinfo && npm run clean-scripts --workspace @tecra/scripts",
        "dev": "concurrently -n renderer,main \"npm run _cra-dev\" \"npm run _main-dev\"",
        "dev:main": "npm run _main-dev",
        "dev:renderer": "npm run _cra-dev",
        "format": "npm run format:root && npm run format:packages",
        "format:packages": "npm run format --workspaces",
        "format:root": "format-code",
        "gen-installer": "electron-builder build --config scripts/electron/electron-builder.json --win --publish never",
        "lint": "npm run lint:root && npm run lint:packages",
        "lint:packages": "npm run lint --workspaces",
        "lint:root": "eslint .",
        "prebuild": "npm run build-scripts && node scripts/npm/md-working-dir.js",
        "predev": "npm run build-scripts && node scripts/npm/md-working-dir.js",
        "run-build": "node scripts/npm/run-build.js",
        "run-unpacked": "node scripts/npm/run-unpacked.js",
        "test": "npm run _cra-test -- --coverage",
        "test-coverage": "npm run _cra-test -- --coverage --watchAll=false"
    },
    "engines": {
        "node": ">=16"
    },
    "workspaces": [
        "packages/eslint-config",
        "packages/scripts",
        "packages/stylelint-config",
        "packages/tsconfigs"
    ],
    "dependencies": {
        "@fluentui/react": "^8.28.2",
        "@reduxjs/toolkit": "^1.6.1",
        "error-oop": "^0.4.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4",
        "react-scrollbars-custom": "^4.0.27",
        "web-vitals": "^2.1.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^7.0.1",
        "@testing-library/user-event": "^13.2.1",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/jest": "^26.0.24",
        "@types/lodash": "^4.14.172",
        "@types/node": "^14.17.9",
        "@types/react": "^17.0.18",
        "@types/react-dom": "^17.0.9",
        "@types/react-redux": "^7.1.18",
        "@types/react-test-renderer": "^17.0.1",
        "@types/speed-measure-webpack-plugin": "^1.3.4",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/experimental-utils": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "@typescript-eslint/scope-manager": "^4.30.0",
        "@typescript-eslint/types": "^4.30.0",
        "@typescript-eslint/typescript-estree": "^4.30.0",
        "@typescript-eslint/visitor-keys": "^4.30.0",
        "browserslist": "^4.16.7",
        "chalk": "^4.1.2",
        "concurrently": "^6.2.1",
        "electron": "13.1.7",
        "electron-builder": "^22.11.7",
        "electron-devtools-installer": "^3.2.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-typescript": "^14.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-deprecation": "^1.2.1",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.25.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "lodash": "^4.17.21",
        "prettier": "2.3.2",
        "react-app-rewired": "^2.1.8",
        "react-scripts": "^4.0.3",
        "react-test-renderer": "^17.0.2",
        "rimraf": "^3.0.2",
        "sass": "^1.38.0",
        "speed-measure-webpack-plugin": "^1.5.0",
        "stylelint": "^13.13.1",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "ts-loader": "^8.3.0",
        "typed-css-modules": "^0.6.8",
        "typescript": "4.4.2",
        "webpack-cli": "^4.8.0"
    }
}
