{
    "name": "template-electron-cra",
    "version": "0.2.0",
    "description": "A template project for developing electron application by electron, react (CRA) and typescript.",
    "main": "build/electron.js",
    "author": {
        "name": "YF1999",
        "email": "1586939317@qq.com",
        "url": "https://github.com/YF1999"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "bugs": {
        "url": "https://github.com/YSoftwareTech/template-electron-cra/issues"
    },
    "config-overrides-path": "configs/webpack.renderer.config.js",
    "eslintConfig": {
        "extends": ".eslintrc.js"
    },
    "homepage": ".",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/YSoftwareTech/template-electron-cra"
    },
    "scripts": {
        "app": "node scripts/app.mjs",
        "build": "yarn build:renderer && yarn build:main",
        "build:main": "webpack --config configs/webpack.main.config.js --mode production",
        "build:renderer": "react-app-rewired build",
        "clean": "rimraf build release working",
        "clean:build": "rimraf build",
        "clean:release": "rimraf release",
        "clean:working": "rimraf working",
        "dev": "concurrently -n renderer,main \"yarn dev:renderer\" \"yarn dev:main\"",
        "dev:main": "webpack --config configs/webpack.main.config.js --mode development",
        "dev:renderer": "react-app-rewired start",
        "installer": "electron-builder build --config configs/electron-builder.yml -w --x64",
        "lint": "eslint src -c .eslintrc.js",
        "prebuild": "node ./scripts/custom-gitconfig.mjs",
        "predev": "node ./scripts/custom-gitconfig.mjs",
        "test": "jest --config configs/jest.config.js",
        "test:react": "react-app-rewired test"
    },
    "dependencies": {
        "@fluentui/react": "^8.0.0-beta.49",
        "@reduxjs/toolkit": "^1.5.0",
        "immer": "^8.0.0",
        "node-errors-pro": "^0.2.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "react-scripts": "4.0.1",
        "react-scrollbars-custom": "^4.0.25"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/jest": "^24.0.0",
        "@types/lodash-es": "^4.17.4",
        "@types/node": "^12.19.9",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-redux": "^7.1.15",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "chalk": "^4.1.0",
        "concurrently": "^5.3.0",
        "electron": "11.2.1",
        "electron-builder": "^22.9.1",
        "electron-devtools-installer": "^3.1.1",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "lodash-es": "^4.17.20",
        "react-app-rewired": "^2.1.6",
        "rimraf": "^3.0.2",
        "speed-measure-webpack-plugin": "^1.3.3",
        "ts-jest": "^26.1.3",
        "ts-loader": "^8.0.1",
        "typescript": "^4.1.3",
        "webpack-cli": "^3.3.12"
    }
}
