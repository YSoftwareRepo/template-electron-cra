{
    "name": "@ter/scripts",
    "version": "0.11.0",
    "description": "Scripts for ter.",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "homepage": "https://github.com/YSoftwareRepo/template-electron-react#readme",
    "license": "MIT",
    "author": "yifanwww <yifanw1101@gmail.com> (https://github.com/yifanwww)",
    "bugs": {
        "url": "https://github.com/YSoftwareRepo/template-electron-react/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YSoftwareRepo/template-electron-react.git",
        "directory": "configs/scripts"
    },
    "bin": {
        "app-main": "./bin/app-main.js",
        "jest": "./node_modules/jest/bin/jest.js",
        "mkdir-working": "./bin/mkdir-working.js",
        "run-build": "./bin/run-build.js",
        "run-unpacked": "./bin/run-unpacked.js",
        "unit-test": "./bin/unit-test.js"
    },
    "scripts": {
        "build-scripts": "concurrently -n cjs,types \"pnpm run build-scripts:cjs\" \"pnpm run build-scripts:types\"",
        "build-scripts:cjs": "tsc --project tsconfigs/tsconfig.cjs.json",
        "build-scripts:types": "tsc --project tsconfigs/tsconfig.types.json",
        "clean": "rimraf dist tsconfigs/*.tsbuildinfo",
        "dev": "pnpm run build-scripts --watch",
        "format": "prettier --write \"**/*.{cjs,css,html,js,jsx,mjs,json,less,scss,ts,tsx,yaml,yml}\"",
        "lint": "eslint .",
        "pre-commit": "lint-staged",
        "typecheck": "tsc --project tsconfig.json --noEmit"
    },
    "dependencies": {
        "@babel/core": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@jest/transform": "^29.5.0",
        "@types/babel__core": "^7.20.1",
        "babel-jest": "^29.5.0",
        "babel-loader": "^9.1.3",
        "babel-preset-react-app": "^10.0.1",
        "camelcase": "^6.3.0",
        "chalk": "^4.1.2",
        "fork-ts-checker-webpack-plugin": "^8.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-resolve": "^29.5.0",
        "jest-watch-typeahead": "^2.2.2",
        "react-app-polyfill": "^3.0.0",
        "semver": "^7.5.4",
        "source-map-loader": "^4.0.1",
        "terser-webpack-plugin": "^5.3.9",
        "text-table": "^0.2.0",
        "vite": "^4.4.0",
        "webpack": "^5.88.1",
        "webpack-cli": "^4.10.0"
    },
    "devDependencies": {
        "@jest/types": "^29.5.0",
        "@ter/eslint-config": "workspace:*",
        "@ter/tsconfigs": "workspace:*",
        "@types/node": "^18.16.18",
        "@types/semver": "^7.5.0",
        "@types/text-table": "^0.2.2",
        "concurrently": "^7.6.0",
        "eslint": "^8.42.0",
        "lint-staged": "^13.2.2",
        "prettier": "2.8.8",
        "rimraf": "^3.0.2",
        "typescript": "5.1.3"
    }
}
